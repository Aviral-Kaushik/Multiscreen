<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Multiscreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PhrasesActivity" android:label="@string/phrases_activity_label" android:exported="true"/>
        <activity android:name=".ColorsActivity" android:label="@string/colors_activity_label" android:exported="true"/>
        <activity android:name=".FamilyActivity" android:label="@string/family_activity_label" android:exported="true"/>
        <activity android:name=".NumbersActivity" android:label="@string/numbers_activity_label" android:exported="true"/>
        <activity android:name=".MainActivity" android:label="@string/main_activity_label" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>